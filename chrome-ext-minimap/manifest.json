{
    "name": "Minimap",
    "description": "Creates a minimap of every site you're on",
    "version": "1.2.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },


    "permissions": [
        "scripting"
      
    ],
    "host_permissions": [
        "<all_urls>"
      ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "/icon.png",
        "32":  "/icon.png",
        "48": "/icon.png",
        "128": "/icon.png"
      }
}