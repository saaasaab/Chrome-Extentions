{
    "manifest_version": 2,
  
    "name": "Daily Calendar",
    "description": "Do something every day",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
      "default_icon": "icon.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "storage",
        "notifications"
    ]
  }