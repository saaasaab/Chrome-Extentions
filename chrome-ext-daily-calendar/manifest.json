{
    "manifest_version": 2,
  
    "name": "Daily Calendat",
    "description": "Do somethin every day",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
      "default_icon": "icon.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["popup.js"]
        }
    ],
    
    "permissions": ["storage"]
  }