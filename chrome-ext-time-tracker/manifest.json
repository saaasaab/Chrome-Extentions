{
    "manifest_version": 2,
  
    "name": "BHAG Tracker",
    "description": "Have to worked toward you BHAG",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "options_page":"options.html",
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["popup.js"]
        }
    ],
    
    "permissions": ["storage"]
  }